<!DOCTYPE html>
<html lang ="it">
 <head>   
<h1>Segnalazioni in archivio </h1>
</head>
<body>

<!-- TODO Usare metodi get dei segnalazioneModel -->
    <% 
        for (var i=0; i<numSegnalazioni; i++){ %>
            <%= listaSegnalazioni[i].nome %>,
            <%= listaSegnalazioni[i].descrizione %>, 
            <%= listaSegnalazioni[i].indirizzo %>,
            <img src=
            <%= listaSegnalazioni[i].img.getPathServer() %> 
            alt="tuo padre" width="100" height="100">
    <% } %>
</body>    
</html>